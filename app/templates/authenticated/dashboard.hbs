<BsModal
  @open={{this.acsModalOpen}}
  @onHidden={{fn (mut this.acsModalOpen) false}}
as |modal|>

  <modal.header>
    <h4 class="modal-title" tabindex="0" aria-label="{{t 'PAGE_OVERVIEW_REMOTE_MANAGEMENT_TITLE'}}">{{t 'PAGE_OVERVIEW_REMOTE_MANAGEMENT_TITLE'}}</h4>
  </modal.header>
  <modal.body>
    <div class="container-fluid text-start">
      <div class="container-fluid text-start">
        {{#each @model.ip.Interface as |interface|}}
        {{#if (eq interface.Alias "mgmt")}}
        {{#each interface.IPv4Address as |ipv4addr|}}
        {{#if (eq ipv4addr.Alias "primary")}}
        <div tabindex="0" aria-label="{{t "PAGE_OVERVIEW_ACS_URL"}}: {{@model.managementserver.URL}}">{{t "PAGE_OVERVIEW_ACS_URL"}}: <a href={{@model.managementserver.URL}}>{{@model.managementserver.URL}}</a></div>
        <div tabindex="0" aria-label="{{t "PAGE_OVERVIEW_MANAGEMENT_IP_ADDRESS"}}: {{ipv4addr.IPAddress}}">{{t "PAGE_OVERVIEW_MANAGEMENT_IP_ADDRESS"}}: {{ipv4addr.IPAddress}}</div>
        {{/if}}
        {{/each}}
        {{/if}}
        {{/each}}
      </div>
    </div>
  </modal.body>

  <modal.footer>
    <BsButton @onClick={{modal.close}}>{{t 'PAGE_OVERVIEW_MODAL_CLOSE'}}</BsButton>
  </modal.footer>

</BsModal>